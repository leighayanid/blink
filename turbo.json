{
  "$schema": "https://turborepo.dev/schema.v2.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".nuxt/**",
        ".output/**",
        "dist/**",
        "build/web/**"
      ],
      "env": ["NODE_ENV"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "test": {
      "cache": false,
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}
